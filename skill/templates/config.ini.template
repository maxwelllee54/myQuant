[data]
# Tushare API Token (必填)
tushare_token = YOUR_TUSHARE_TOKEN_HERE

# 股票代码列表，用逗号分隔
# A股示例: 000001.SZ,600000.SH
# 美股示例: AAPL,MSFT,GOOGL
stock_codes = 000001.SZ,600000.SH

# 数据起止日期 (格式: YYYYMMDD)
start_date = 20200101
end_date = 20231231

# 数据输出目录
output_dir = ./data

[features]
# 特征输出目录
feature_dir = ./features

[models]
# 模型类型: RandomForest, XGBoost, LSTM, etc.
model_type = RandomForest

# 模型保存目录
model_dir = ./models

[reports]
# 报告输出目录
report_dir = ./reports
